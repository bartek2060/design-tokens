{
  "__settings": {
    "utilityBreakpoints": "breakpoints"
  },
  "colors": {
    "primary": {
      "__settings": {
        "generateUtility": {
          "className": "bg",
          "property": "background-color"
        }
      },
      "main": "#1976d2",
      "light": "#42a5f5",
      "dark": "#1565c0"
    },
    "text": {
      "__settings": {
        "generateUtility": {
          "className": "text",
          "property": "color"
        }
      },
      "primary": "#000000",
      "secondary": "#666666"
    }
  },
  "spacing": {
    "__settings": {
      "generateUtility": [
        {
          "className": "m",
          "property": "margin"
        },
        {
          "className": "ml",
          "property": "margin-left"
        },
        {
          "className": "mr",
          "property": "margin-right"
        },
        {
          "className": "mt",
          "property": "margin-top"
        },
        {
          "className": "mb",
          "property": "margin-bottom"
        },
        {
          "className": "mx",
          "property": ["margin-left", "margin-right"]
        },
        {
          "className": "my",
          "property": ["margin-top", "margin-bottom"]
        }
      ]
    },
    "small": "0.5rem",
    "medium": "1rem",
    "large": "2rem"
  },
  "breakpoints": {
    "mobile": "320px",
    "tablet": "768px",
    "desktop": "1024px"
  },
  "typography": {
    "fontFamily": {
      "primary": "Arial, sans-serif",
      "secondary": "Georgia, serif"
    },
    "fontSize": {
      "small": "0.875rem",
      "medium": "1rem",
      "large": "1.25rem"
    }
  }
}
